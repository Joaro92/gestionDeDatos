<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="WindowsFormsApplication1" namespace="WindowsFormsApplication1" xmlns="urn:nhibernate-mapping-2.2">
  <class name="SoloSubasta" table="SoloSubastas" lazy="true" >
    
    <id name="rowID" column="rowID" />
    
    <property name="idUsuario">
      <column name="idUsuario" sql-type="int" not-null="true" />
    </property>
    <property name="tipoCompra">
      <column name="tipoCompra" sql-type="varchar" not-null="true" />
    </property>
    <property name="fecha">
      <column name="fecha" sql-type="datetime" not-null="false" />
    </property>
    <property name="valorInicialVenta">
      <column name="valorInicialVenta" sql-type="numeric" not-null="true" />
    </property>
    <property name="montoOferta">
      <column name="montoOferta" sql-type="numeric" not-null="false" />
    </property>
    <property name="adjudicada">
      <column name="adjudicada" sql-type="bit" not-null="false" />
    </property>
    <property name="codigoPublicacion">
      <column name="codigoPublicacion" sql-type="int" not-null="false" />
    </property>
    <property name="descripcion">
      <column name="descripcion" sql-type="varchar" not-null="false" />
    </property>
  </class>
</hibernate-mapping>