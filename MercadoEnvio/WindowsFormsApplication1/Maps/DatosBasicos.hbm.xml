<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="WindowsFormsApplication1" namespace="WindowsFormsApplication1" xmlns="urn:nhibernate-mapping-2.2">
  <class name="DatosBasicos" table="DatosBasicos" dynamic-insert="true" dynamic-update="true"  >

    <id name="idDatosBasicos" column="idDatosBasicos">
      <generator class="identity" />
    </id>
    <property name="email">
      <column name="email" sql-type="varchar" not-null="true" />
    </property>
    <property name="telefono">
      <column name="telefono" sql-type="varchar" not-null="false" />
    </property>
    <property name="domCalle">
      <column name="domCalle" sql-type="varchar" not-null="false" />
    </property>
    <property name="nroCalle">
      <column name="nroCalle" sql-type="int" not-null="false" />
    </property>
    <property name="piso">
      <column name="piso" sql-type="int" not-null="false" />
    </property>
    <property name="depto">
      <column name="depto" sql-type="varchar" not-null="false" />
    </property>
    <property name="codPostal">
      <column name="codPostal" sql-type="varchar" not-null="false" />
    </property>
    <property name="localidad">
      <column name="localidad" sql-type="varchar" not-null="false" />
    </property>
    <property name="ciudad">
      <column name="ciudad" sql-type="varchar" not-null="false" />
    </property>

  </class>
</hibernate-mapping>