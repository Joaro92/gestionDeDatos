<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="WindowsFormsApplication1" namespace="WindowsFormsApplication1" xmlns="urn:nhibernate-mapping-2.2">
  <class name="EstadisticaVendedores" table="EstadisticaVendedores" lazy="true" >
    
    <id name="rowID" column="rowID" />
    
    <property name="idPublicacion">
      <column name="idPublicacion" sql-type="int" not-null="true" />
    </property>
    <property name="idUsuario">
      <column name="idUsuario" sql-type="int" not-null="true" />
    </property>
    <property name="nombre">
      <column name="nombre" sql-type="varchar" not-null="false" />
    </property>
    <property name="fechaCreacion">
      <column name="fechaCreacion" sql-type="date" not-null="true" />
    </property>
    <property name="stock">
      <column name="stock" sql-type="int" not-null="false" />
    </property>
    <property name="idVisibilidad">
      <column name="idVisibilidad" sql-type="int" not-null="true" />
    </property>
    <property name="costo">
      <column name="costo" sql-type="numeric" not-null="true" />
    </property>
  </class>
</hibernate-mapping>