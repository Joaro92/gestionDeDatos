    <id name="rowID" column="rowID" />
    
    <property name="idUsuario">
      <column name="idUsuario" sql-type="int" not-null="true" />
    </property>
    <property name="tipoCompra">
      <column name="tipoCompra" sql-type="varchar" not-null="true" />
    </property>
    <property name="fecha">
      <column name="fecha" sql-type="datetime" not-null="false" />
    </property>
    <property name="compraCantidad">
      <column name="compraCantidad" sql-type="int" not-null="true" />
    </property>
    <property name="codigoPublicacion">
      <column name="codigoPublicacion" sql-type="int" not-null="false" />
    </property>
    <property name="descripcion">
      <column name="descripcion" sql-type="varchar" not-null="false" />
    </property>
    <property name="cantEstrellas">
      <column name="cantEstrellas" sql-type="int" not-null="false" />
    </property>
    <property name="descripcionCalificacion">
      <column name="descripcionCalificacion" sql-type="varchar" not-null="false" />
    </property>
  </class>
</hibernate-mapping>