<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="WindowsFormsApplication1" namespace="WindowsFormsApplication1" xmlns="urn:nhibernate-mapping-2.2">
  <class name="Rubro" table="Rubro" lazy="true" >

    <id name="idRubro" column="idRubro">
      <generator class="identity" />
    </id>
    
    <property name="codigo">
      <column name="codigo" sql-type="nchar" not-null="false" />
    </property>
    
    <property name="nombreCorto">
      <column name="nombreCorto" sql-type="nvarchar" not-null="false" />
    </property>
    
    <property name="descripcion">
      <column name="descripcion" sql-type="nvarchar" not-null="false" />
    </property>
    
  </class>
</hibernate-mapping>